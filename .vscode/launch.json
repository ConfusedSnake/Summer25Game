{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Kj√∏r SFML-app",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/sfml-app",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "DYLD_LIBRARY_PATH",
          "value": "${workspaceFolder}/dependencies/SFML/lib"
        }
      ],
      "externalConsole": true,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Aktiver auto-visning av typer",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
